

@{
    ViewData["Title"] = "Home page";
}

<head>
    <meta charset="utf-8" />
    @*<script type="text/javascript" src="lib/signalr.min.js"></script>*@
    <script src="https://code.jquery.com/jquery-3.2.1.js" integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js" integrity="sha256-T0Vest3yCU7pafRw9r+settMBX6JkKN06dqBnpQ8d30="
            crossorigin="anonymous"></script>
    <title>WhiteBoard</title>
    <style>

        canvas {
            cursor: crosshair;
            border: 1px solid #000000;
        }
    </style>
</head>
@using Microsoft.AspNetCore.Identity

@inject SignInManager<ApplicationUser> SignInManager
@if (SignInManager.IsSignedIn(User))
{
    <h2>     </h2>

    <body>
        <canvas id="canvas" width="1150" height="500" onmouseout="clearMousePositions()"></canvas>
        <select id="color">
            <option value="black">Black</option>
            <option value="red">Red</option>
            <option value="yellow">Yellow</option>
            <option value="green">Green</option>
            <option value="blue">Blue</option>
        </select>


        <img id="imgSave" style="border: 0; border-style: dashed; width: 32px; height: 32px"
             src="data:image/png;base64," onclick="SaveDrawings(); return false;" />

        <form id="drawingForm" method="post" asp-controller="Notes" asp-action="Create">

            <input type="hidden" name="imageData" id="imageData" />
            <div class="form-group">
                @*<label asp-for="Text" class="control-label"></label>*@
                <input placeholder="Take a Note!" name="note" type="text" class="form-control" />
                @*<span asp-validation-for="Text" class="text-danger"></span>*@
            </div>

        </form>
        @*<div id="output"></div>*@

    </body>
}
else
{
    <h1>Welcome To Live-Board!</h1>
    <h2>Please sign in to begin sharing</h2>

}
<script src="~/lib/signalr/dist/browser/signalr.js"></script>
<script src="~/js/whiteBoard.js"></script>